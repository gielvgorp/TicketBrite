{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:vue/vue3-essential"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "vue"
    ],
    "rules": {
        "no-unused-vars": [
            "off"
        ],
        "no-console": [
            "off"
        ],
        "no-undef": "off",
        "no-explicit-any": "off",
        "no-unsafe-argument": "off",
        "no-unsafe-assignment": "off",
        "no-unsafe-call": "off",
        "no-unsafe-return": "off"
    }
}